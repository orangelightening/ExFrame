  "plugins": [
    {
      "plugin_id": "generalist",
      "name": "EEFrame Guide",
      "description": "Expert in EEFrame usage and features",
      "module": "plugins.generalist",
      "class": "GeneralistPlugin",
      "enabled": true,
      "config": {
        "name": "EEFrame",
        "keywords": [
          "how", "what", "where", "when", "why", "use", "create", "find", "search",
          "pattern", "domain", "query", "survey", "universe", "start", "begin", "help",
          "eeframe", "system", "interface", "navigate", "understand", "learn"
        ],
        "categories": ["getting_started", "features", "how_to", "concepts"],
        "threshold": 0.25
      }
    }
  ],

  "specialists": [
    {
      "specialist_id": "generalist",
      "name": "EEFrame Guide",
      "description": "Expert in EEFrame usage and features",
      "expertise_keywords": [
        "how", "what", "where", "when", "why", "use", "create", "find", "search",
        "pattern", "domain", "query", "survey", "universe", "start", "begin", "help",
        "eeframe", "system", "interface", "navigate", "understand", "learn",
        "question", "problem", "solve", "answer", "ask"
      ],
      "expertise_categories": ["getting_started", "features", "how_to", "concepts", "troubleshooting"],
      "confidence_threshold": 0.25,
      "response_template": "structured"
    }
  ],

  "enrichers": [
    {
      "module": "plugins.enrichers.llm_enricher",
      "class": "LLMFallbackEnricher",
      "enabled": true,
      "config": {
        "mode": "fallback",
        "min_confidence": 0.30,
        "model": "glm-4.7",
        "max_patterns": 5,
        "research_strategy": {
          "type": "document",
          "use_chunking": false,
          "base_path": "/app/universes/default/docs",
          "documents": [
            {"type": "file", "path": "user-guide.md"},
            {"type": "file", "path": "context.md"},
            {"type": "file", "path": "README.md"}
          ]
        }
      }
    }
  ],

  "categories": ["getting_started", "features", "how_to", "concepts", "troubleshooting"],
  "tags": ["help", "tutorial", "guide", "documentation", "learn"],

  "ui_config": {
    "placeholder_text": "How can I help you learn EEFrame? (Press Enter to submit)",
    "example_queries": [
